package be.bastien.ecran;

public class Acceuil {

	public static void main(String[] args) {
		EventQueue.invokeLater(new Runnable() {
			public void run() {
				try {
					Acceuil acceuil = new Acceuil();
					acceuil.setTitle("Acceuil");
					acceuil.setVisible(true);
				} catch (Exception e) {
					e.printStackTrace();
				}
			}
		});
	}

}
